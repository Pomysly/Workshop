%table.table
	%tr
		%th Picture Link
		%th
	- product.pictures.each do |picture|
		%tr
			%td= picture.url
			%td= link_to 'Delete picture', category_product_picture_path(product.category, product, picture), method: :delete, data: { confirm: 'Are you sure?' }
%br
= render 'form'

